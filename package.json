{
  "name": "node-tesseract-ocr",
  "version": "0.1.0",
  "private": true,
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:addon": "node-gyp rebuild --debug",
    "build:cjs": "tsc -p tsconfig.cjs.json && mv dist/cjs/index.js dist/cjs/index.cjs",
    "build:esm": "tsc -p tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "build": "rm -rf dist && mkdir dist && npm run build:addon && npm run build:cjs && npm run build:esm",
    "build:examples": "npm run build && npx tsc -p tsconfig.examples.json",
    "example:recognize": "npm run build:examples && node dist/examples/recognize.js"
  },
  "gypfile": true,
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@types/node": "^22.0.0",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^10.0.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "bindings": "^1.5.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./lib/index.d.ts"
    }
  }
}
